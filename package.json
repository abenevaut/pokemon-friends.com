        "copy-webpack-plugin": "^5.1.1",
        "cross-env": "^7.0.2",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "^14.1.0",
        "eslint-loader": "^3.0.3",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-lodash": "^6.0.0",
        "eslint-plugin-no-jquery": "^2.3.2",
